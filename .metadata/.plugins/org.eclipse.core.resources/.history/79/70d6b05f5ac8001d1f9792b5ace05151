package pnu.edu.service;

import java.util.ArrayList;
import java.util.Date;
import java.util.List;

import org.springframework.stereotype.Service;

import pnu.edu.domain.Member;

@Service
public class MemberService {

	
	public MemberService() {
		
	}
	
	public List<Member> getMembers1(){
		List<Member> list = new ArrayList<>();
		list.add(new Member(1, "1234", "홍길동", new Date()));
		list.add(new Member(2, "1235", "홍길동2", new Date()));
		list.add(new Member(3, "1236", "홍길동3", new Date()));
		list.add(new Member(4, "1237", "홍길동4", new Date()));
		list.add(new Member(5, "1238", "홍길동5", new Date()));
		
		return list;
	}
	
}
